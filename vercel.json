{
  "version": 2,
  "framework": "vite",
  "buildCommand": "npm --prefix Frontend install --no-audit --no-fund && npm --prefix Frontend run build",
  "outputDirectory": "Frontend/dist",
  "functions": {
    "backend/server.js": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/Frontend/dist/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}