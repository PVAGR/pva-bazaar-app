{
  "version": 2,
  "framework": "vite",
  "buildCommand": "node ./scripts/vercel-build-wrapper.cjs",
  "outputDirectory": "dist",
  
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}